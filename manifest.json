{
	"manifest_version": 2,

	"name": "Pyco report",
	"description": "This is the extension to extract report data from Pyco system",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "report.html"
	},
	"background": {
		"persistent": false,
		"scripts": ["background.js"]
	},
	"permissions": ["tabs", "storage", "https://jira.pyramid-consulting.com/*"],
	"web_accessible_resources": ["jquery.js", "jira-extract.js", "template.js"]
}
